#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>


int main() {
    int num, first, last, sum;
    scanf("%d", &num);
    last = num % 10;
    first = num / 1000;
    sum = first + last;
    printf("%d\n", sum);



    return 0;
}
