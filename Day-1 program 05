Decimal Formatting

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    float a;
    scanf("%f",&a);
    printf("%0.5f\n",a);
     printf("%0.4f\n",a);
     printf("%0.3f\n",a);
     printf("%0.2f\n",a);
     printf("%0.1f\n",a);
     printf("%d\n",(int)a);
    return 0;
    
}
