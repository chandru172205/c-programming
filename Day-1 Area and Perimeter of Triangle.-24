#include <stdio.h>

int main() {
    float base, height, side1, side2, side3;
    
    scanf("%f", &base);
    scanf("%f", &height);
    scanf("%f", &side1);
    scanf("%f", &side2);
    scanf("%f", &side3);

    float area = 0.5 * base * height;
    float perimeter = side1 + side2 + side3;

    printf("Area of Triangle is %.2f\n", area);
    printf("Perimeter of Triangle is %.2f\n", perimeter);

    return 0;
}
