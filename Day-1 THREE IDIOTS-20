#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    float x1, y1, x2, y2;
    float mid_x, mid_y;
    scanf("%f", &x1);
    scanf("%f", &y1);
    scanf("%f", &x2);
    scanf("%f", &y2);
    mid_x = (x1 + x2) / 2;
    mid_y = (y1 + y2) / 2;
    printf("Binoy's house is located at (%.1f,%.1f)\n", mid_x, mid_y);
    return 0;
}
